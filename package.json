{
  "name": "wazimap-ng",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Adi Eyal <adi@openup.org.za>",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "utils": "^0.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "cssnano": "^4.1.10",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-data-src": "^1.0.1",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "shx": "^0.3.3"
  },
  "scripts": {
    "start": "parcel ./src/index.html",
    "prebuild-dev": "shx rm -rf dist/*",
    "build-dev": "parcel build ./src/index.html ./src/embed-example.html --no-minify --public-url ./",
    "prebuild": "shx rm -rf dist/*",
    "build": "parcel build ./src/index.html ./src/embed-example.html --public-url ./ ",
    "push-prod": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose",
    "push-staging": "push-dir --remote=staging --dir=dist --branch=gh-pages --cleanup --verbose",
    "clean-files": "cp dist/js*.js dist/idp.js;",
    "test": "jest"
  },
  "browserslist": "> 0.25%, not dead",
  "importWebflowExport": {
    "importHtml": [
      {
        "glob": "*.html",
        "destDir": "src",
        "transforms": "./src/js/webflow/import.js"
      }
    ]
  },
  "staticFiles": {
    "staticPath": {
      "staticPath": [
        "src/documents/lottieflow-menu-nav-08-ffffff-easey.json",
        "src/documents/001-understanding-the-idp.pdf",
        "src/documents/002-understanding-the-phases-of-the-idp.pdf"
      ],
      "staticOutDir": "documents"
    }
  }
}
